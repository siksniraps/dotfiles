# Increase volume
"pamixer -i 5 && volnoti-show $(pamixer --get-volume)"
   XF86AudioRaiseVolume

# Decrease volume
"pamixer -d 5 && volnoti-show $(pamixer --get-volume)"
   XF86AudioLowerVolume
   
# Toggle mute
"pamixer -t; if pamixer --get-mute; then volnoti-show -m; else volnoti-show $(pamixer --get-volume); fi"
   XF86AudioMute    
